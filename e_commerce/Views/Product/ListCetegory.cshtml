@model IEnumerable<e_commerce.EF.Cetegory>

@{
    ViewBag.Title = "ListCetegory";
}

<h2>ListCetegory</h2>

<p>
    @Html.ActionLink("Create New", "Cetegory")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>Modify</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Delete", "DeleteCetegory", new { id=item.id }, new { @class = "btn btn-default" })
        </td>
    </tr>
}

</table>
